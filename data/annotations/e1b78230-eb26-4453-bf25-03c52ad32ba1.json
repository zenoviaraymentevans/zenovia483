      "start_time": 161,
      "end_time": 162,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "And so"
            }
          ]
        }
      ],
      "tags": [
        {
          "category": "_uncategorized_",
          "tag": "Transcription"
        }
      ],
      "uuid": "a142f3ad-8b46-41a7-9bf0-ed360dd9b06e"
    },
    {
      "start_time": 163,
      "end_time": 164,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "naturally"
            }
          ]
        }
      ],
