    },
    {
      "start_time": 150,
      "end_time": 151,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "I rise"
            }
          ]
        }
      ],
      "tags": [
        {
          "category": "_uncategorized_",
          "tag": "Transcription"
        },
        {
          "category": "_uncategorized_",
          "tag": "Tempo"
        }
      ],
      "uuid": "9e802912-d448-4ad0-96af-4c45069e96dd"
    },
    {
      "start_time": 152,
      "end_time": 155,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Bringing the gifts that my ancestors gave,"
            }
          ]
        }
      ],
      "tags": [
        {
          "category": "_uncategorized_",
          "tag": "Transcription"
        },
        {
          "category": "_uncategorized_",
          "tag": "Tempo"
        }
      ],
      "uuid": "90e2a1f4-d1c2-4451-bf99-575973af7ed8"
    },
    {
      "start_time": 156,
      "end_time": 160,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "I am the dream and the hope of the slave."
            }
          ]
        }
      ],
      "tags": [
        {
          "category": "_uncategorized_",
          "tag": "Transcription"
        }
      ],
      "uuid": "4a64ca4e-c7d7-4031-adf5-2e8c250385b4"
    },
